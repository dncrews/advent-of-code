{
  "extends": "vstg",
  "rules": {
    "arrow-body-style": "off",
    "arrow-parens": "off",
    "newline-after-var": "off",
    "newline-before-return": "off",
    "no-cond-assign": "off",
    "no-console": "off",
    "no-extra-parens": "off",
    "no-magic-numbers": "off",
    "curly": "off"
  }
}
